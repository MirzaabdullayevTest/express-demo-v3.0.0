doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title= title 
    body 
        h1(style="color: red", class="title") #{title}

        - let books = ['Rich dad and pood dad', 'Rework', 'Marketing 5.0']
        ul.list 
            each book in books
                li= book 